"exit\n"
"style\n"
"config\n"
"submenu\n"
"restart\n"
"reconfig\n"
"stylesdir / stylesmenu\n"
"workspaces"
msgstr
"les différents types sont :\n"
"nop\n"
"exec\n"
"exit\n"
"style\n"
"config\n"
"submenu\n"
"restart\n"
"reconfig\n"
"stylesdir / stylesmenu\n"
"workspaces"

